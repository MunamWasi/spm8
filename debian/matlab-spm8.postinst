#!/bin/sh
set -e

case "$1" in
    configure)
        debian-matlab-mexhelper spm8 install \
            --build-cmd 'make MEXBIN="matlab-mex"'
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

